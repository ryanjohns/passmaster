%html
  %head
    %meta{ 'http-equiv' => 'Content-Type', :content => 'text/html; charset=utf-8' }
    %meta{ :name => 'viewport', :content => 'initial-scale=1.0, maximum-scale=1.0, user-scalable=no' }
    %title Passmaster - Accounts Viewer
    :css
      table {
        border-collapse: collapse;
      }
      th {
        padding: 3px 5px;
      }
      pre {
        margin: 3px 5px;
      }

    :javascript
      // helper to sort account objects by account name
      function compareByAccountName(a, b) {
        var nameA = a['account'].toUpperCase();
        var nameB = b['account'].toUpperCase();

        if (nameA < nameB) {
          return -1;
        }
        if (nameA > nameB) {
          return 1;
        }

        return 0;
      };

      // called when the form is submitted
      function formSubmitted() {
        // read the password
        var password = document.getElementById('backup_password').value;
        if (password.length == 0) {
          alert('Please enter a Backup Password.');
          return;
        }

        // use a FileReader object to read the contents of the backup file
        var file = document.getElementById('backup_file').files[0];
        if (typeof file == 'undefined') {
          alert('Please select a Backup File.');
          return;
        }

        var reader = new FileReader();
        reader.onload = (function(evt) {
          // call the decrypt function passing in the supplied password and the contents of the backup file
          decryptBackup(password, evt.target.result);
        });
        reader.readAsText(file);
      };

      // decrypts the backupString using password
      function decryptBackup(password, backupString) {
        // the backup is JSON-encoded so attempt to parse it
        var backup;
        try {
          backup = JSON.parse(backupString);
        } catch(err) {
          alert('Failed to load backup file.')
          return;
        }

        // take the SHA-256 hash of the plain text password and use it as the actual password for decryption
        var hashedPassword = sjcl.codec.hex.fromBits(sjcl.hash.sha256.hash(password));

        // the encrypted data is base64 encoded so attempt to decode it
        var ciphertext;
        try {
          ciphertext = Base64.decode(backup['encrypted_data']);
        } catch(err) {
          alert('Failed to decode backup.');
          return;
        }

        // use the hashed password to do the decryption
        var decrypted;
        try {
          decrypted = sjcl.decrypt(hashedPassword, ciphertext);
        } catch(err) {
          alert('Failed to decrypt accounts.');
          return;
        }

        // JSON parse the decrypted data to get the accounts object
        var accounts;
        try {
          accounts = JSON.parse(decrypted);
        } catch(err) {
          alert('Failed to parse accounts.');
          return;
        }

        // display the accounts
        showAccounts(accounts);
      };

      // fills the table with accounts
      function showAccounts(accounts) {
        var accountsAry = [];
        for (accountId in accounts) {
          accountsAry.push(accounts[accountId]);
        }
        accountsAry.sort(compareByAccountName);

        var table = document.getElementById('accounts_table');
        var html = '';

        var columns = [];
        for (var j = 0; j < accountsAry.length; j++) {
          var account = accountsAry[j];

          if (columns.length == 0) {
            html += '<tr>';
            for (column in account) {
              columns.push(column);
              html += '<th>' + column + '</th>';
            }
            html += '</tr>';
          }

          html += '<tr>';
          for (var i = 0; i < columns.length; i++) {
            var str = account[columns[i]];

            // html-escape the string
            var div = document.createElement('div');
            div.appendChild(document.createTextNode(str));
            var escaped_str = div.innerHTML;

            html += '<td><pre>' + escaped_str + '</pre></td>';
          }
          html += '</tr>';
        }

        table.innerHTML = html;
      };

  %body
    %h1 Passmaster - Accounts Viewer
    %p This tool can be used to decrypt and view the accounts from a backup file.
    %p It works without internet access and serves as documentation of the host-proof encryption scheme.
    %p **NOTE** This does not sync with the Passmaster servers and you cannot add, edit, or delete accounts.

    %form#backup_form{ :method => 'POST', :enctype => 'multipart/form-data', :onsubmit => 'formSubmitted(); return false;' }
      .field
        %label{ :for => 'backup_file' } Backup File
        %input#backup_file{ :name => 'file', :type => 'file', :accept => 'text/plain' }
      .field
        %label{ :for => 'backup_password' } Backup Password
        %input#backup_password{ :name => 'password', :type => 'password' }
      .field
        %input{ :type => 'submit', :value => 'View Accounts' }

    %hr

    %table#accounts_table{ :border => '1' }

